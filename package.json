{
  "name": "bike-rental-management-system",
  "version": "1.0.0",
  "description": "Bike Rental Management System with Next.js frontend and Express.js backend",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=backend",
    "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "bike-rental",
    "management-system",
    "nextjs",
    "expressjs",
    "mongodb",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.5.0",
    "react-chartjs-2": "^5.3.0",
    "recharts": "^3.1.2"
  }
}
